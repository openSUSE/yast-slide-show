msgid "Web and Scripting Module"
msgstr "Web とスクリプティングに関するモジュール"

#: slideshow.xml:219(para)
msgid "Legacy Module"
msgstr "従来のモジュール"

#: slideshow.xml:224(para)
msgid "Toolchain Module"
msgstr "ツールチェインモジュール"

#: slideshow.xml:229(para)
msgid "Public Cloud Module"
msgstr "パブリッククラウドモジュール"

